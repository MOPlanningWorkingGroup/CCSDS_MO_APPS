  <itemDefinition id="_procedureExecutionServiceProvider" structureRef="org.ccsds.moims.mo.mal.automation.provider.ProcedureExecutionServiceProvider" />
  <itemDefinition id="_procedureExecutionServiceConsumer" structureRef="org.ccsds.moims.mo.mal.automation.consumer.ProcedureExecutionServiceConsumer" />
  <itemDefinition id="_procedureDefinition" structureRef="org.ccsds.moims.mo.automation.procedureexecution.structures.ProcedureDefinition" />
  <itemDefinition id="_defId" structureRef="Long" />

  <escalation id="null" escalationCode="null" />
  <process processType="Private" isExecutable="true" id="org.ccsds.moims.mo.mal.automation" name="Hello World" >

    <!-- process variables -->
    <property id="procedureExecutionServiceProvider" itemSubjectRef="_procedureExecutionServiceProvider"/>
    <property id="procedureExecutionServiceConsumer" itemSubjectRef="_procedureExecutionServiceConsumer"/>
    <property id="procedureDefinition" itemSubjectRef="_procedureDefinition"/>
    <property id="defId" itemSubjectRef="_defId"/>

    <!-- nodes -->
    <startEvent id="_1"  name="start" isInterrupting="true"/>
    <endEvent id="_3" >
        <terminateEventDefinition />
    </endEvent>
    <scriptTask id="_jbpm-unique-3" name="Add Procedure Definition" scriptFormat="http://www.java.com/java" >
      <script>kcontext.setVariable("defId", procedureExecutionServiceConsumer.getProcedureExecutionService().addDefinition(procedureDefinition));</script>
    </scriptTask>
    <scriptTask id="_jbpm-unique-5" name="Log Id" >
      <script>System.out.println("procedureDefinition id is " + defId);</script>
    </scriptTask>
    <endEvent id="_jbpm-unique-6" name="Error" >
      <escalationEventDefinition escalationRef="null" />
    </endEvent>
    <exclusiveGateway id="_jbpm-unique-8" name="Gateway" gatewayDirection="Diverging" />
    <scriptTask id="_jbpm-unique-11" name="Log Error" >
      <script>System.out.println("error occured");</script>
    </scriptTask>
    <scriptTask id="_jbpm-unique-12" name="Log Result" >
      <script>System.out.println("successful test");</script>
    </scriptTask>

    <!-- connections -->
    <sequenceFlow id="_jbpm-unique-12-_3" sourceRef="_jbpm-unique-12" targetRef="_3" />
    <sequenceFlow id="_1-_jbpm-unique-3" sourceRef="_1" targetRef="_jbpm-unique-3" />
    <sequenceFlow id="_jbpm-unique-3-_jbpm-unique-5" sourceRef="_jbpm-unique-3" targetRef="_jbpm-unique-5" />
    <sequenceFlow id="_jbpm-unique-11-_jbpm-unique-6" sourceRef="_jbpm-unique-11" targetRef="_jbpm-unique-6" />
    <sequenceFlow id="_jbpm-unique-5-_jbpm-unique-8" sourceRef="_jbpm-unique-5" targetRef="_jbpm-unique-8" />
    <sequenceFlow id="_jbpm-unique-8-_jbpm-unique-11" sourceRef="_jbpm-unique-8" targetRef="_jbpm-unique-11" name="defId is NULL" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" >return defId == null;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_jbpm-unique-8-_jbpm-unique-12" sourceRef="_jbpm-unique-8" targetRef="_jbpm-unique-12" name="defId is not NULL" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" >return defId != null;</conditionExpression>
    </sequenceFlow>

  </process>

