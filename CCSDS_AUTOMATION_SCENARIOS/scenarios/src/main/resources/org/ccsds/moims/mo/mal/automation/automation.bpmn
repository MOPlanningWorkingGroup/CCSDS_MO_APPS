      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="_jbpm-unique-5" >
        <dc:Bounds x="360" y="264" width="100" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="_jbpm-unique-6" >
        <dc:Bounds x="211" y="523" width="48" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="_jbpm-unique-8" >
        <dc:Bounds x="387" y="344" width="48" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="_jbpm-unique-11" >
        <dc:Bounds x="198" y="435" width="80" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="_jbpm-unique-12" >
        <dc:Bounds x="565" y="439" width="80" height="48" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="_jbpm-unique-12-_3" >
        <di:waypoint x="605" y="463" />
        <di:waypoint x="610" y="556" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="_1-_jbpm-unique-3" >
        <di:waypoint x="407" y="62" />
        <di:waypoint x="408" y="175" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="_jbpm-unique-3-_jbpm-unique-5" >
        <di:waypoint x="408" y="175" />
        <di:waypoint x="410" y="288" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="_jbpm-unique-11-_jbpm-unique-6" >
        <di:waypoint x="238" y="459" />
        <di:waypoint x="235" y="547" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="_jbpm-unique-5-_jbpm-unique-8" >
